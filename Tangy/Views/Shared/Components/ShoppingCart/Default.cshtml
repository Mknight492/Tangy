@model int
@{
    ViewData["Title"] = "Default";
}

@if (Model > 0)
{
    <li style="color: white;">
        <a asp-controller="Cart" asp-action="Index" class="nav-link">
            <i class="fas fa-shopping-cart"></i>
            &nbsp;
             (@(Model))   Shopping Cart
        </a>
    </li>
}